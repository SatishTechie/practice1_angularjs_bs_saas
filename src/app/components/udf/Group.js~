define([
        "dojo/_base/declare",
        "dijit/_WidgetBase",
        "dijit/_TemplatedMixin",
        "dijit/_WidgetsInTemplateMixin",
        "dijit/_OnDijitClickMixin",
        "dojo/text!./template/group.html",
        "dojo/_base/lang",
        "dojo/topic"
    ],
    function(declare, _WidgetBase, _TemplatedMixin, _OnDijitClickMixin,
        _WidgetsInTemplateMixin, template, lang, topic) {
        return declare(
            "Group", [_WidgetBase, _TemplatedMixin, _OnDijitClickMixin,
                _WidgetsInTemplateMixin
            ], {

                templateString: template,
                constructor: function(options) {
                    lang.mixin(this, options);

                },
                postCreate: function() {

                    var me = this;
                    for (var i = __app.jsonview.length - 1; i >= 0; i--) {
                        var items = __app.jsonview[i].components;
                        for (var j = 0; j < items.length; j++) {
                            var item = items[j];
                            for (var key in item) {
                                console.log(key);
                            }
                        }
                    }

                }

            });
    });
